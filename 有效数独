```
func isValidSudoku(board [][]byte) bool {
    n := 9
    var row [9][9]int
    var col [9][9]int
    var grid [3][3][9]int
    for i := 0; i < n; i ++ {
        for j := 0; j < n; j++ {
            if board[i][j] == '.' {
                continue
            }
            num := board[i][j] - '1'
            row[i][num]++
            col[j][num]++
            grid[i/3][j/3][num]++
            if row[i][num] > 1 || col[j][num]>1 || grid[i/3][j/3][num] > 1{
                return false
            }
        }
    }
    return true
}
```
